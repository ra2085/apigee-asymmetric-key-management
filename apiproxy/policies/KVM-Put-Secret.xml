<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations async="false" continueOnError="false" enabled="true" name="KVM-Put-Secret" mapIdentifier="JWTKeyPairs">
    <DisplayName>KVM-Put-Secret</DisplayName>
    <Properties/>
    <ExclusiveCache>false</ExclusiveCache>
    <ExpiryTimeInSecs>-1</ExpiryTimeInSecs>
    <Put override="true">
        <Key>
            <Parameter>gwjwt_key_pair</Parameter>
        </Key>
        <Value ref="private.key_pair"/>
    </Put>
    <Put override="true">
        <Key>
            <Parameter>gwjwt_pem_public_key</Parameter>
        </Key>
        <Value ref="private.jwt_public_key"/>
    </Put>
    <Put override="true">
        <Key>
            <Parameter>gwjwt_pem_private_key</Parameter>
        </Key>
        <Value ref="private.jwt_private_key"/>
    </Put>
    <Scope>environment</Scope>
</KeyValueMapOperations>