<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations async="false" continueOnError="false" enabled="true" name="KVM-Get-Secret" mapIdentifier="JWTKeyPairs">
    <DisplayName>KVM-Get-Secret</DisplayName>
    <Properties/>
    <ExclusiveCache>false</ExclusiveCache>
    <Get assignTo="private.jwt_private_key">
        <Key>
            <Parameter>gwjwt_pem_public_key</Parameter>
        </Key>
    </Get>
    <Get assignTo="private.jwt_public_key">
        <Key>
            <Parameter>gwjwt_pem_private_key</Parameter>
        </Key>
    </Get>
    <Scope>environment</Scope>
</KeyValueMapOperations>